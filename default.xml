<?xml version="1.0" encoding="UTF-8"?>
<manifest>
	<remote 
		name="aosp"
		fetch="https://android.googlesource.com/"
		review="https://android-review.googlesource.com/" />
	<default
		revision="master"
		remote="aosp"
		sync-j="4" />

	<!--Tools-->
	<project path="ndk" name="platform/ndk" remote="aosp" />

	<!--Toolchain sources	-->
	<project path="toolchain/binutils" name="toolchain/binutils" remote="aosp" />

	<!--Prebuilts-->
	<project path="prebuilts/gcc/linux-x86/host/x86_64-linux-glibc2.15-4.8" name="platform/prebuilts/gcc/linux-x86/host/x86_64-linux-glibc2.15-4.8" remote="aosp" />
	<project path="platform/prebuilts/clang/host/linux-x86" name="platform/prebuilts/clang/host/linux-x86" remote="aosp" />

	<!--Qemu source-->
	<project path="external/qemu" name="platform/external/qemu" remote="aosp" />
	<extend-project path="external/qemu" revision="emu-3.1-release" />
</manifest>

